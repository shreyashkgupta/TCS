    "Author: Shreyash Gupta"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "metadata": {},
   "outputs": [],
   "source": [
    "def reverse_string(inp):\n",
    "    inp_list = list(inp)\n",
    "    inp_list.reverse()\n",
    "    return \"\".join(inp_list)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Enter string: Hello please reverse this\n",
      "siht esrever esaelp olleH\n"
     ]
    }
   ],
   "source": [
    "string = input(\"Enter string: \")\n",
    "print(reverse_string(string))"
   ]
  }
